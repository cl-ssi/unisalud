{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "actived/microsoft-teams-notifier": "^1.3",
        "awcodes/filament-gravatar": "^2.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "calebporzio/sushi": "^2.4",
        "filament/filament": "^3.2",
        "laravel/framework": "^11.44.1",
        "league/commonmark": "^2.7",
        "laravel/tinker": "^2.9",
        "owen-it/laravel-auditing": "^13.6",
        "psr/simple-cache": "2.0",
        "pxlrbt/filament-environment-indicator": "^2.0",
        "pxlrbt/filament-excel": "^2.3",
        "ryangjchandler/blade-capture-directive": "1.0 as 0.3",
        "socialiteproviders/claveunica": "^4.2",
        "spatie/laravel-google-cloud-storage": "^2.3",
        "spatie/laravel-permission": "^6.9",
        "stackkit/laravel-google-cloud-tasks-queue": "^4.2",
        "stechstudio/filament-impersonate": "^3.11",
        "yemenopensource/filament-excel": "^0.1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-google-static-map.git"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
